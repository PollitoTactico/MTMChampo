<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MTMChampo.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MTMChampo.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="MTMChampo">

    <ShellContent Title="Home" ContentTemplate="{DataTemplate local:HomeH}" Route="HomeH" />
    <ShellContent Route="login" ContentTemplate="{DataTemplate local:Inicio}" />
    <ShellContent Route="register" ContentTemplate="{DataTemplate local:Register}" />

    <TabBar>
        
        <ShellContent Title="Cuentas" Icon="bank.png" ContentTemplate="{DataTemplate local:CuentasBancariasPage}" />

        
        <ShellContent Title="About" Icon="bank.png" ContentTemplate="{DataTemplate local:About}" />
    </TabBar>


</Shell>
